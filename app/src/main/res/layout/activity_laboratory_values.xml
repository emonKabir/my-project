<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Laboratory.LaboratoryValues"
    android:orientation="vertical"
    >


    <include
        android:id="@+id/tool_bar"
        layout="@layout/toolbar"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:drawableLeft="@drawable/ic_flasks"
        android:drawablePadding="5dp"
        android:text="@string/laboratory_values"
        android:textColor="@color/blood_request_tColor"
        android:textSize="25sp" />


    <ListView
        android:id = "@+id/listview"
        android:layout_width = "match_parent"
        android:layout_height = "match_parent"
        android:divider="@color/laboratoryListviewColor"
        android:dividerHeight="0.5sp"


        />


</LinearLayout>